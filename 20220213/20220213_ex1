{
        // 單行註解 Console::WriteLine(L"Hello World");
	/* 多行註解
	std::count << "Please input score : "; //畫面輸出字串
	std::cin   >> a; //鍵盤輸入整數
	std::count << a;
	*/
	count << "Please input score : "; //畫面輸出字串
	cin   >> a; //鍵盤輸入整數
